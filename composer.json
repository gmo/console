{
	"name": "gmo/console",
	"description": "A library that eases the creation of beautiful and testable command line interfaces",
	"require": {
		"symfony/console": "~2.5",
		"symfony/event-dispatcher": "~2.5"
	},
	"require-dev": {
		"gmo/dependency-injection": "~1.0"
	},
	"suggest": {
		"gmo/dependency-injection": "Provides container aware commands"
	},
	"autoload": {
		"psr-4": {
			"GMO\\Console\\": "src/"
		}
	},
	"repositories": [
		{ "type": "composer", "url": "composer.gmodev.com" }
	]
}
